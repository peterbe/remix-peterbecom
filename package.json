{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run parcel:build && remix build",
    "parcel:build": "parcel build --dist-dir app/styles/build app/*.css",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "dev:node": "cross-env NODE_ENV=development esrun --watch ./server.ts ",
    "dev:remix": "remix watch",
    "dev:parcel": "parcel watch --dist-dir app/styles/build app/global.css",
    "start": "cross-env NODE_ENV=production esrun ./server.ts",
    "tsc": "tsc",
    "lint": "eslint app",
    "lintfix": "eslint app --fix && npm run prettier:write",
    "test": "curl --fail -s http://localhost:3000 > /dev/null && ava",
    "test:watch": "ava --watch",
    "prettier:write": "prettier --write app *.ts",
    "prettier:check": "prettier --check app *.ts"
  },
  "dependencies": {
    "@digitak/esrun": "3.2.24",
    "@picocss/pico": "1.5.10",
    "@remix-run/express": "1.19.1",
    "@remix-run/node": "1.19.1",
    "@remix-run/react": "1.19.1",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "express": "4.18.2",
    "express-async-handler": "1.2.0",
    "http-proxy-middleware": "2.0.6",
    "isbot": "3.6.13",
    "morgan": "1.10.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollbar": "2.26.2",
    "shrink-ray-current": "4.1.3",
    "swr": "2.2.0",
    "throttle-debounce": "5.0.0",
    "tiny-invariant": "1.3.1",
    "usehooks-ts": "2.9.1"
  },
  "devDependencies": {
    "@remix-run/dev": "1.19.1",
    "@remix-run/eslint-config": "1.19.1",
    "@types/morgan": "1.9.4",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "@types/throttle-debounce": "5.0.0",
    "ava": "5.3.1",
    "cheerio": "1.0.0-rc.12",
    "dotenv": "16.3.1",
    "eslint": "8.46.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "nodemon": "3.0.1",
    "npm-run-all": "4.1.5",
    "parcel": "2.9.3",
    "prettier": "3.0.0",
    "sharp": "0.32.4",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": ">=14"
  }
}
